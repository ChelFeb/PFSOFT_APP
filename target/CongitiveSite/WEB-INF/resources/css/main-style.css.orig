.starter-template {
    padding: 40px 15px;
    text-align: center;
}

.errorMessage {
    color: red;
}

.logout {
    position: absolute;
    top: 0;
    right: 0;
    margin-right: 20px;
    margin-top: 5px;
}

.form-signin {
    max-width: 380px;
    padding: 15px;
    margin: 0 auto;
}

.form-signin .form-signin-heading,
.form-signin .checkbox {
    margin-bottom: 10px;
}

.form-signin .checkbox {
    font-weight: normal;
}

.form-signin .form-control {
    position: relative;
    height: auto;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    padding: 10px;
    font-size: 15px;
}

.form-signin .form-control:focus {
    z-index: 2;
}

.form-signin input[type="email"] {
    margin-bottom: -1px;
    border-bottom-right-radius: 0;
    border-bottom-left-radius: 0;
}

.form-signin input[type="password"] {
    margin-bottom: 10px;
    border-top-left-radius: 0;
    border-top-right-radius: 0;
}

#footer {
    position: fixed;
    width: 100%;
    bottom: 0;
    background-color: #f5f5f5;
    margin: 0 auto;
}

#footerText {
    text-align: center;
}

.table {
    display: table;
}
.table-caption{
    display:table-caption;
    width: 100%;
    padding: 0;
    margin-bottom: 20px;
    font-size: 21px;
    line-height: inherit;
    color: #333;
    border: 0;
    border-bottom: 1px solid #e5e5e5;
}
.table-header-group {
    display: table-row;
}
.table-row {
    display: table-row;
}
.table-cell {
    display: table-cell;
}

.tooltip.top .tooltip-arrow {
    bottom: 0;
    left: 50%;
    margin-left: -5px;
    border-top-color: #000000;
    border-width: 5px 5px 0;
}
.tooltip.right .tooltip-arrow {
    width: auto;
    max-width: 800px;
    top: 50%;
    left: 0;
    margin-top: -5px;
    border-right-color: #000000;
    border-width: 5px 5px 5px 0;
}
.tooltip-inner {
    color: #000;
    background: #f9f9f9;
    border: solid 1px #000000;
}

.box {
    display: inline-block;
    background-color: #f9f9f9;
    border: solid 1px #000000;
    border-radius: 20px;
    margin-bottom: 2px;
    margin-top: 2px;
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 1px;
    padding-bottom: 1px;
}

